    <!-- https://gitlab.com/azihsan/dislocation-ontology/-/raw/master/crystal-structure-ontology.owl/hasBasis -->

    <owl:ObjectProperty rdf:about="https://gitlab.com/azihsan/dislocation-ontology/-/raw/master/crystal-structure-ontology.owl/hasBasis">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://gitlab.com/azihsan/dislocation-ontology/-/raw/master/crystal-structure-ontology.owl/CoordinateVectorOfBasis"/>
                    <rdf:Description rdf:about="https://gitlab.com/azihsan/dislocation-ontology/-/raw/master/crystal-structure-ontology.owl/Lattice"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/mdo/structure/Basis"/>
        <rdfs:comment xml:lang="en">hasBasis represents the relationship between a lattice or coordinate vector basis and basis.</rdfs:comment>
        <rdfs:label xml:lang="en">has basis</rdfs:label>
    </owl:ObjectProperty>
    


