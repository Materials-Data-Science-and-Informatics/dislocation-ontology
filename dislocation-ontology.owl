                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/diso#SlipPlane"/>
                    <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/diso#SlipSystem"/>
                </owl:unionOf>
