            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty crystal:translationVectorConstantV ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty crystal:translationVectorConstantW ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty crystal:translationVectorConstantV ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty crystal:translationVectorConstantW ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty crystal:translationVectorConstantU ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

mdo:Site a owl:Class ;
    rdfs:label "Site"@en ;
    rdfs:comment "A site is a point in a lattice."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] ) ] .

mdo:Species a owl:Class ;
    rdfs:label "Species"@en ;
    rdfs:comment "A species is the combination of atoms on each site."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass chebi:CHEBI_33250 ;
            owl:onProperty mdo:hasElement ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass chebi:CHEBI_33250 ;
            owl:onProperty mdo:hasElement ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass chebi:CHEBI_33250 ;
            owl:onProperty mdo:hasElement ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

crystal:SlipPlaneNormal a owl:Class ;
    rdfs:label "Slip Plane Normal"@en ;
    rdfs:comment "Unit normal vector of slip planes"@en .

mdo:Basis a owl:Class ;
    rdfs:label "Basis"@en ;
    rdfs:comment "A basis defines a spatial unit used to express fractional coordinates."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:unionOf ( [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onClass crystal:LatticeParameterAngle ;
                                    owl:onProperty crystal:hasLatticeParameterAngle ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                                    owl:onClass crystal:LatticeParameterLength ;
                                    owl:onProperty crystal:hasLatticeParameterLength ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] [ a owl:Restriction ;
                        owl:onClass mdo:AxisVectors ;
                        owl:onProperty mdo:hasAxisVectors ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        [ a owl:Class ;
            owl:unionOf ( [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onClass crystal:LatticeParameterAngle ;
                                    owl:onProperty crystal:hasLatticeParameterAngle ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                                    owl:onClass crystal:LatticeParameterLength ;
                                    owl:onProperty crystal:hasLatticeParameterLength ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] [ a owl:Restriction ;
                        owl:onClass mdo:AxisVectors ;
                        owl:onProperty mdo:hasAxisVectors ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        [ a owl:Class ;
            owl:unionOf ( [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onClass crystal:LatticeParameterAngle ;
                                    owl:onProperty crystal:hasLatticeParameterAngle ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                                    owl:onClass crystal:LatticeParameterLength ;
                                    owl:onProperty crystal:hasLatticeParameterLength ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] [ a owl:Restriction ;
                        owl:onClass mdo:AxisVectors ;
                        owl:onProperty mdo:hasAxisVectors ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

mdo:Lattice a owl:Class ;
    rdfs:label "Lattice"@en ;
    rdfs:comment "A lattice defines a periodic arrangement of one or more atoms."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticePlane ;
            owl:someValuesFrom crystal:LatticePlane ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasBasis ;
                        owl:someValuesFrom mdo:Basis ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Basis ;
                        owl:onProperty crystal:hasBasis ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticePoint ;
            owl:someValuesFrom crystal:LatticePoint ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticePoint ;
            owl:someValuesFrom crystal:LatticePoint ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticePoint ;
            owl:someValuesFrom crystal:LatticePoint ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasUnitCell ;
                        owl:someValuesFrom crystal:UnitCell ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:UnitCell ;
                        owl:onProperty crystal:hasUnitCell ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasUnitCell ;
                        owl:someValuesFrom crystal:UnitCell ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:UnitCell ;
                        owl:onProperty crystal:hasUnitCell ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticeDirection ;
            owl:someValuesFrom crystal:LatticeDirection ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticeDirection ;
            owl:someValuesFrom crystal:LatticeDirection ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticeDirection ;
            owl:someValuesFrom crystal:LatticeDirection ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasUnitCell ;
                        owl:someValuesFrom crystal:UnitCell ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:UnitCell ;
                        owl:onProperty crystal:hasUnitCell ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasBasis ;
                        owl:someValuesFrom mdo:Basis ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Basis ;
                        owl:onProperty crystal:hasBasis ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticePlane ;
            owl:someValuesFrom crystal:LatticePlane ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasLatticePlane ;
            owl:someValuesFrom crystal:LatticePlane ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasBasis ;
                        owl:someValuesFrom mdo:Basis ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Basis ;
                        owl:onProperty crystal:hasBasis ] ) ] .

crystal:UnitCell a owl:Class ;
    rdfs:label "Unit Cell"@en ;
    rdfs:comment "Basic structural unit or building block of the crystal structure and defines the crystal structure by virtue of its geometry and the atom positions within."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty crystal:hasCrystalSystem ;
            owl:someValuesFrom crystal:CrystalSystem ],
        [ a owl:Restriction ;
            owl:allValuesFrom crystal:LatticeParameterLength ;
            owl:onProperty crystal:hasLatticeParameterLength ],
        [ a owl:Restriction ;
            owl:allValuesFrom crystal:LatticeParameterAngle ;
            owl:onProperty crystal:hasLatticeParameterAngle ],
        [ a owl:Restriction ;
            owl:allValuesFrom crystal:LatticeParameterAngle ;
            owl:onProperty crystal:hasLatticeParameterAngle ],
        [ a owl:Restriction ;
            owl:allValuesFrom crystal:LatticeParameterLength ;
            owl:onProperty crystal:hasLatticeParameterLength ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasCrystalSystem ;
            owl:someValuesFrom crystal:CrystalSystem ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasCrystalSystem ;
            owl:someValuesFrom crystal:CrystalSystem ],
        [ a owl:Restriction ;
            owl:allValuesFrom crystal:LatticeParameterLength ;
            owl:onProperty crystal:hasLatticeParameterLength ],
        [ a owl:Restriction ;
            owl:allValuesFrom crystal:LatticeParameterAngle ;
            owl:onProperty crystal:hasLatticeParameterAngle ] .

crystal:SlipDirection a owl:Class ;
    rdfs:label "Slip Direction"@en ;
    rdfs:comment "The direction in the slip plane corresponding to one of the shortest lattice translation vectors."@en ;
    rdfs:subClassOf crystal:LatticeDirection .

crystal:SlipPlane a owl:Class ;
    rdfs:label "Slip Plane"@en ;
    rdfs:comment "Slip plane is normally the plane with the highest density of atoms, i.e. those which are most widely spaced."@en,
        "The crystallographic plane along which the dislocation line traverses."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasSlipPlaneNormal ;
                        owl:someValuesFrom crystal:SlipPlaneNormal ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:SlipPlaneNormal ;
                        owl:onProperty crystal:hasSlipPlaneNormal ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipDirection ;
            owl:someValuesFrom crystal:SlipDirection ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasSlipPlaneNormal ;
                        owl:someValuesFrom crystal:SlipPlaneNormal ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:SlipPlaneNormal ;
                        owl:onProperty crystal:hasSlipPlaneNormal ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipDirection ;
            owl:someValuesFrom crystal:SlipDirection ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasSlipPlaneNormal ;
                        owl:someValuesFrom crystal:SlipPlaneNormal ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:SlipPlaneNormal ;
                        owl:onProperty crystal:hasSlipPlaneNormal ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipDirection ;
            owl:someValuesFrom crystal:SlipDirection ],
        crystal:LatticePlane .

crystal:SlipSystem a owl:Class ;
    rdfs:label "Slip System"@en ;
    rdfs:comment "Slip system is defined as the set of slip planes with the same unit normal vector and the same slip direction."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipPlaneNormal ;
            owl:someValuesFrom crystal:SlipPlaneNormal ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipPlaneNormal ;
            owl:someValuesFrom crystal:SlipPlaneNormal ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipDirection ;
            owl:someValuesFrom crystal:SlipDirection ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipPlaneNormal ;
            owl:someValuesFrom crystal:SlipPlaneNormal ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipDirection ;
            owl:someValuesFrom crystal:SlipDirection ],
        [ a owl:Restriction ;
            owl:onProperty crystal:hasSlipDirection ;
            owl:someValuesFrom crystal:SlipDirection ] .

mdo:CoordinateVector a owl:Class ;
    rdfs:label "Coordinate Vector"@en ;
    rdfs:comment "A coordinate vector is the coordinates of a point in space."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:X_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Y_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Y_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:X_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Z_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Z_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Z_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:X_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Y_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

