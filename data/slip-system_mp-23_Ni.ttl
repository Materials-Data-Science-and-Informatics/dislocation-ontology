crystal:CrystallineMaterial a owl:Class ;
    rdfs:label "Kristall"@de,
        "Crystalline Material"@en ;
    rdfs:comment "Crystalline materials are characterized by a periodic arrangement of the constituent atoms."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasCrystallineDefect ;
                        owl:someValuesFrom crystal:CrystallineDefect ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:CrystallineDefect ;
                        owl:onProperty crystal:hasCrystallineDefect ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasCrystallineDefect ;
                        owl:someValuesFrom crystal:CrystallineDefect ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:CrystallineDefect ;
                        owl:onProperty crystal:hasCrystallineDefect ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasCrystalStructure ;
                        owl:someValuesFrom crystal:CrystalStructure ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:CrystalStructure ;
                        owl:onProperty crystal:hasCrystalStructure ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasCrystalStructure ;
                        owl:someValuesFrom crystal:CrystalStructure ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:CrystalStructure ;
                        owl:onProperty crystal:hasCrystalStructure ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasCrystallineDefect ;
                        owl:someValuesFrom crystal:CrystallineDefect ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:CrystallineDefect ;
                        owl:onProperty crystal:hasCrystallineDefect ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasCrystalStructure ;
                        owl:someValuesFrom crystal:CrystalStructure ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:CrystalStructure ;
                        owl:onProperty crystal:hasCrystalStructure ] ) ] .

crystal:Dislocation a owl:Class ;
    rdfs:label "Versetzung"@de,
        "Dislocation"@en ;
    rdfs:comment "A line object that is a boundary separating the regions on the slip plane which have undergone slip from those that have not."@en,
        "Linear or one-dimensional defect around which some of the atoms are misaligned."@en,
        "Presence of dislocation introduces local disturbance of the atomic-level geometry."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasBurgersVector ;
                        owl:someValuesFrom crystal:BurgersVector ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:BurgersVector ;
                        owl:onProperty crystal:hasBurgersVector ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasLineSense ;
                        owl:someValuesFrom crystal:LineSense ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:LineSense ;
                        owl:onProperty crystal:hasLineSense ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:movesOn ;
            owl:someValuesFrom crystal:SlipPlane ],
        [ a owl:Restriction ;
            owl:onProperty crystal:movesOn ;
            owl:someValuesFrom crystal:SlipPlane ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasLineSense ;
                        owl:someValuesFrom crystal:LineSense ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:LineSense ;
                        owl:onProperty crystal:hasLineSense ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasLineSense ;
                        owl:someValuesFrom crystal:LineSense ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:LineSense ;
                        owl:onProperty crystal:hasLineSense ] ) ],
        [ a owl:Restriction ;
            owl:onProperty crystal:movesOn ;
            owl:someValuesFrom crystal:SlipPlane ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasBurgersVector ;
                        owl:someValuesFrom crystal:BurgersVector ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:BurgersVector ;
                        owl:onProperty crystal:hasBurgersVector ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty crystal:hasBurgersVector ;
                        owl:someValuesFrom crystal:BurgersVector ] [ a owl:Restriction ;
                        owl:allValuesFrom crystal:BurgersVector ;
                        owl:onProperty crystal:hasBurgersVector ] ) ],
        crystal:CrystallineDefect .

